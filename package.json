{
  "name": "lvc-app",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/main/main.js",
  "author": "lvc",
  "description": "lol voice chat app",
  "homepage": ".",
  "sideEffects": false,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "inko": "^1.1.1",
    "league-connect": "^6.0.0-rc12",
    "mediasoup-client": "^3.6.100",
    "node-global-key-listener": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "redis": "^4.6.10",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.0.7",
    "typescript": "^4.9.5",
    "uiohook-napi": "^1.5.2",
    "web-vitals": "^2.1.4",
    "ws": "^8.14.2"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "clean": "rm -rf dist",
    "electron-start": "npm run clean && tsc && electron .",
    "start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000\" \"npm run electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy:xos": "electron-builder --mac"
  },
  "build": {
    "extends": null,
    "productName": "lvc",
    "appId": "com.lvc-app",
    "asar": true,
    "mac": {
      "target": [
        "default"
      ]
    },
    "dmg": {
      "title": "lvc"
    },
    "directories": {
      "buildResources": "./build/",
      "output": "./release/build/"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json",
      "build/**/*",
      {
        "from": "dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.46",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "assert": "^2.0.0",
    "concurrently": "^8.2.1",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
